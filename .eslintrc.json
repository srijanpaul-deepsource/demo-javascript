{
  "extends": ["airbnb", "standard-with-typescript", "eslint:all", "standard"],
  "plugins": ["node", "security"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "accessor-pairs": "off",
    "for-direction": "off"
  },
  "globals": {
    "foobar": "off",
    "PromiseMe": "readonly",
    "badvalue": null,
    "worsevalue": false,
    "wtf": true
  }
}
